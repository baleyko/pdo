{
      "name": "baleyko/pdo"
    , "description": "The library to extend some PDO functionality and make it more friendly"
    , "type": "library"
    , "license": "MIT"
    , "keywords": [
          "php"
        , "library"
        , "database"
        , "pdo"
    ]
    , "authors": [
        {
              "name": "Valery Baleyko"
            , "email": "baleyko@gmail.com"
            , "role": "lead"
        }
    ]
    , "require": {
          "php": ">=5.5.0"
        , "ext-PDO": "*"
    }
    , "require-dev": {
          "phpunit/phpunit": "~4.4"
        , "squizlabs/php_codesniffer": "~2.0"
        , "satooshi/php-coveralls": "~0.6"
    }
    , "autoload": {
        "psr-4": {
            "PDO\\": "src/"
        }
    }
    , "extra": {
        "branch-alias": {
            "dev-master": "develop"
        }
    }
    , "minimum-stability": "dev"
}
